pipeline {
  agent { label '!go'}
  stages  {
    agent { label '!go'}
    stage('Create docker-agent'){
        agent{
            dockerfile{
                filename 'Docker'
            }
        }
        steps {
            sh 'curl http://google.com | wc -c >google-size'
            sh 'cat google-size'
        }
    }
  }
}
